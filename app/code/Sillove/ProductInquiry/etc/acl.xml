<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Sillove_ProductInquiry::productinquiry_config" title="Product Inquiry"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog">
                    <resource id="Sillove_ProductInquiry::view" title="Product Inquiry" translate="title" sortOrder="10">
                        <resource id="Sillove_ProductInquiry::add_inquiry" title="Manage Inquiries" sortOrder="10" />
                        <resource id="Sillove_ProductInquiry::configration" title="Configuration" sortOrder="20" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
